CHIP ShiftRight{
	IN in[16];
    OUT out[16];
     
    PARTS:
	// Put you code here:
    And(a=in[15] ,b=in[15] ,out=msb);
    Mux16(a[15]=msb,a[0..14]=in[1..15], b=in,sel=false,out=out);
}